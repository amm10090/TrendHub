{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "no-undef": "error",
    "no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "overrides": [
    {
      "files": ["**/pages/**/*.js"],
      "rules": {
        "no-unused-vars": "off",
        "react/display-name": "off",
        "react/prop-types": "off"
      }
    }
  ]
}
