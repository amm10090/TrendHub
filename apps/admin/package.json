{
  "name": "@trend-hub/admin",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "clean": "rm -rf .next node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:push": "prisma db push",
    "db:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/lib/db/seed.ts",
    "db:start:win": "pg_ctl -D \"C:/Program Files/PostgreSQL/14/data\" start",
    "db:stop:win": "pg_ctl -D \"C:/Program Files/PostgreSQL/14/data\" stop",
    "db:start:unix": "sudo service postgresql start",
    "db:stop:unix": "sudo service postgresql stop",
    "start:all:win": "npm run db:start:win && npm run dev",
    "start:all:unix": "npm run db:start:unix && npm run dev"
  },
  "dependencies": {
    "@heroui/react": "2.7.6-beta.2",
    "@heroui/theme": "2.4.12",
    "@hookform/resolvers": "^4.1.3",
    "@prisma/client": "6.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.5.0",
    "lucide-react": "^0.482.0",
    "next": "^15.2.3",
    "next-intl": "^4.0.2",
    "pg": "8.14.1",
    "prisma": "6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "2.15.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "winston": "3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@next/eslint-plugin-next": "^15.2.3",
    "@tailwindcss/postcss": "4.0.14",
    "@tanstack/react-query": "5.68.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "postcss": "8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.14",
    "ts-node": "10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm src/lib/db/seed.ts"
  }
}
