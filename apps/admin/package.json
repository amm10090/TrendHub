  "scripts": {
    "dev": "next dev --port 3001",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "clean": "rm -rf .next node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:start:win": "pg_ctl -D \"C:/Program Files/PostgreSQL/14/data\" start",
    "db:stop:win": "pg_ctl -D \"C:/Program Files/PostgreSQL/14/data\" stop",
    "db:start:unix": "sudo service postgresql start",
    "db:stop:unix": "sudo service postgresql stop",
    "start:all:win": "npm run db:start:win && npm run dev",
    "start:all:unix": "npm run db:start:unix && npm run dev"
  },
